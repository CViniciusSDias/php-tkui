{
    "name": "skoro/php-gui",
    "description": "Create desktop applications using native gui",
    "keywords": [
        "gui", "ui", "desktop", "toolkit"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Skorobogatko Alexei",
            "email": "skorobogatko.oleksii@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "ext-spl": "*",
        "psr/log": "^1.1",
        "monolog/monolog": "^2.2",
        "m1/env": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "PhpGui\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "PhpGui\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-f": "phpunit --filter"
    }
}
