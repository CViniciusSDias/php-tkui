{
    "name": "skoro/php-tk",
    "description": "Provides api to Tcl/Tk and UI classes.",
    "type": "library",
    "authors": [
        {
            "name": "Skorobogatko Alexei",
            "email": "skorobogatko.oleksii@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "ext-spl": "*",
        "psr/log": "^1.1",
        "monolog/monolog": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "TclTk\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "TclTk\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-f": "phpunit --filter"
    }
}
